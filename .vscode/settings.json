{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.quickSuggestions": {
    "strings": true
  },
  // eslint格式化匹配文件
  "eslint.validate": ["javascript", "javascriptreact", "vue", "typescript", "typescriptreact"],
  "files.associations": {
    "pages.json": "jsonc",
    "manifest.json": "jsonc"
  },
  "path-intellisense.mappings": {
    "@/": "${workspaceRoot}/src"
  },
  "unocss.root": "./"
}
